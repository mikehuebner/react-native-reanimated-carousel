{
  "name": "@mikehuebner/react-native-reanimated-carousel",
  "version": "1.0.2",
  "description": "Simple carousel component. Fully implemented using Reanimated. Infinitely scrolling, very smooth.",
  "author": "Doho <zhaodonghao586@outlook.com> (https://github.com/dohooo)",
  "license": "MIT",
  "homepage": "https://github.com/mikehuebner/react-native-reanimated-carousel#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikehuebner/react-native-reanimated-carousel.git"
  },
  "bugs": {
    "url": "https://github.com/mikehuebner/react-native-reanimated-carousel/issues"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "react-native": "src/index.tsx",
  "types": "lib/typescript",
  "source": "src/index.tsx",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "jest run src/**/*",
    "test:dev": "jest dev src/**/* --watch",
    "typescript": "tsc --noEmit",
    "dev": "watch 'pnpm prepare' ./src",
    "prepare": "bob build",
    "clean": "del-cli lib"
  },
  "peerDependencies": {
    "@types/react": "*",
    "react": ">=18.2.0",
    "react-native": ">=0.74.1",
    "react-native-gesture-handler": ">=2.16.0",
    "react-native-reanimated": ">=3.10.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18",
    "babel-plugin-module-resolver": "^5.0.2",
    "del-cli": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "pod-install": "^0.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.74.1",
    "react-native-builder-bob": "^0.23.2",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-reanimated": "3.12.0",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0",
    "watch": "^1.0.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "configFile": "../../babel.config.js"
        }
      ],
      "module",
      [
        "typescript",
        {
          "tsc": "../../node_modules/.bin/tsc"
        }
      ]
    ]
  }
}
